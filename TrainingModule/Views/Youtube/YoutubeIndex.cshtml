@model TrainingModule.Models.Youtube
@{
    ViewData["Title"] = "Youtube Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h4>Video materials from Youtube</h4>
@foreach (var item in Model.items)
{
    <img src="@item.snippet.thumbnails.medium.url" />
    <p>
        <a href="https://www.youtube.com/watch/@item.id.videoId">@item.snippet.title"</a>
    </p>
        <br />
        
    
}